<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/back_gaaame"
    android:layout_height="match_parent"
    tools:context=".gaaaaame.fraaau.ChooseImageFragment">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.35" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_80"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.80" />



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_60"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.60" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vert_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.10" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vert_70"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.70" />



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vert_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.10" />




    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_70"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.70" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.17" />




    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.25" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_95"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.30" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vert_95"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vert_50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.50" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vert_90"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.90" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_choose_avatar"
        android:fontFamily="@font/lobste_reg"
        android:layout_height="wrap_content"
        android:textAllCaps="true"
        android:layout_width="wrap_content"
        android:textSize="39sp"
        android:gravity="center"
        android:lines="1"
        android:textColor="#C29FF3"
        android:maxLines="1"
        android:text="choose avatar"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_95"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_05"
        app:layout_constraintTop_toTopOf="@id/guideline_hor_05" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_choose_avatar_1"
        android:layout_height="wrap_content"
        android:textAllCaps="true"
        android:layout_width="wrap_content"
        android:textSize="12sp"
        android:gravity="center"
        android:lines="1"
        android:textColor="#C29FF3"
        android:maxLines="1"
        android:text="* your logo for this game"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_95"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_05"
        app:layout_constraintTop_toBottomOf="@id/tv_choose_avatar" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/user_logo"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_05"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_95"
        app:layout_constraintTop_toBottomOf="@id/tv_choose_avatar_1"
        android:src="@drawable/ic_baseline_question_mark_24"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/img_1"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_05"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_50"
        app:layout_constraintBottom_toTopOf="@id/img_2"
        app:layout_constraintTop_toBottomOf="@id/user_logo"
        android:src="@drawable/g4"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/img_2"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_05"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_50"
        app:layout_constraintTop_toBottomOf="@id/img_1"
        app:layout_constraintBottom_toTopOf="@id/img_3"
        android:src="@drawable/g2"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/img_3"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_05"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_50"
        app:layout_constraintTop_toBottomOf="@id/img_2"
        app:layout_constraintBottom_toTopOf="@id/img_4"
        android:src="@drawable/g3"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/img_4"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_05"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_50"
        app:layout_constraintTop_toBottomOf="@id/img_3"
        app:layout_constraintBottom_toTopOf="@id/btn_okkkk_app"
        android:src="@drawable/g1"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/img_5"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_50"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_95"
        app:layout_constraintBottom_toTopOf="@id/img_2"
        app:layout_constraintTop_toBottomOf="@id/user_logo"
        android:src="@drawable/g5"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/img_6"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_50"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_95"
        app:layout_constraintTop_toBottomOf="@id/img_1"
        app:layout_constraintBottom_toTopOf="@id/img_3"
        android:src="@drawable/g6"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/img_7"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_50"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_95"
        app:layout_constraintTop_toBottomOf="@id/img_2"
        app:layout_constraintBottom_toTopOf="@id/img_4"
        android:src="@drawable/g7"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/img_8"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_50"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_95"
        app:layout_constraintTop_toBottomOf="@id/img_3"
        app:layout_constraintBottom_toTopOf="@id/btn_okkkk_app"
        android:src="@drawable/g8"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_okkkk_app"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_black"
        android:text="confirm"
        app:layout_constraintVertical_chainStyle="packed"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@id/guideline_hor_95"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_95"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_05"
        />





</androidx.constraintlayout.widget.ConstraintLayout>